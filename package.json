{
  "name": "freedom",
  "version": "1.0.0",
  "description": "this is a site that I built to help shoppers have an easier experience finding exactly what they're looking for.",
  "main": "server/startup.js",
  "scripts": {
    "start": "node server/startup.js",
    "dev": "nodemon server/startup.js",
    "validate-config": "node validate-config.js",
    "deploy": "bash deploy.sh",
    "db": "node scripts/db-cli.js",
    "db:migrate": "node scripts/db-cli.js migrate",
    "db:rollback": "node scripts/db-cli.js rollback",
    "db:seed": "node scripts/db-cli.js seed",
    "db:status": "node scripts/db-cli.js status",
    "db:health": "node scripts/db-cli.js health",
    "db:migrate-data": "node scripts/db-cli.js migrate-data",
    "db:migration-status": "node scripts/db-cli.js migration-status",
    "db:reset": "node scripts/db-cli.js reset",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:prod": "docker-compose up --build",
    "docker:prod:down": "docker-compose down",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "docker:setup": "bash docker-setup.sh",
    "docker:validate": "bash validate-docker.sh",
    "cloud:deploy:aws": "bash cloud/aws/deploy-aws.sh",
    "cloud:deploy:do": "bash cloud/digitalocean/deploy-do.sh",
    "cloud:check:aws": "bash cloud/aws/deploy-aws.sh check",
    "cloud:check:do": "bash cloud/digitalocean/deploy-do.sh check",
    "cloud:readiness": "bash cloud-readiness-check.sh",
    "monitoring:start": "bash monitoring/scripts/monitoring-stack.sh start",
    "monitoring:stop": "bash monitoring/scripts/monitoring-stack.sh stop",
    "monitoring:restart": "bash monitoring/scripts/monitoring-stack.sh restart",
    "monitoring:status": "bash monitoring/scripts/monitoring-stack.sh status",
    "monitoring:health": "bash monitoring/scripts/monitoring-stack.sh health",
    "monitoring:info": "bash monitoring/scripts/monitoring-stack.sh info",
    "monitoring:logs": "bash monitoring/scripts/monitoring-stack.sh logs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "express-slow-down": "^2.0.3",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "puppeteer": "^21.11.0",
    "rate-limiter-flexible": "^5.0.3",
    "winston": "^3.17.0",
    "winston-cloudwatch": "^3.2.0"
  }
}
